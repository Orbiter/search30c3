<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#ffc6e8ec" />
      <stroke
          android:width="1dp"
          android:color="#ffffffff" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
    <item android:state_enabled="false" >
    <shape>
      <solid
          android:color="#ffaadde3" />
      <stroke
          android:width="1dp"
          android:color="#ffffffff" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
	<shape>
      <gradient
          android:startColor="#ff0099ab"
          android:endColor="#ff007785"
          android:angle="270" />
	  <stroke
          android:width="1dp"
          android:color="#ffffffff" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>